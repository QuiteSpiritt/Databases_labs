ADD UCR VARCHAR(100),
    DCR DATETIME,
    ULC VARCHAR(100),
    DLC DATETIME;

ALTER TABLE Speaker
ADD UCR VARCHAR(100),
    DCR DATETIME,
    ULC VARCHAR(100),
    DLC DATETIME;

ALTER TABLE Presentation
ADD UCR VARCHAR(100),
    DCR DATETIME,
    ULC VARCHAR(100),
    DLC DATETIME;

ALTER TABLE Equipment
